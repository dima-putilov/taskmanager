<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dmstaskmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/my_calendar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/my_calendar_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".maintask"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
<!--
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
-->
        </activity>
        <activity
            android:name=".Flat"
            android:label="@string/title_activity_flat"
            android:parentActivityName=".maintask"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreditActivity"
            android:label="@string/title_activity_credit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CreditItemActivity"
            android:parentActivityName=".CreditActivity" />
        <activity android:name=".PaymentActivity" />
        <activity
            android:name=".GraphicActivity"
            android:parentActivityName=".CreditActivity" />
        <activity android:name=".GraphicItemActivity" />

        <activity android:name=".main"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>